{
	"env": {
		"es6": false,
		"node": true
	},

	"plugins": [],

	"ecmaFeatures": {
	},

	"globals": {
	},

	"rules": {
		"camelcase": 0,
		"comma-dangle": 0,
		"dot-notation": 0,
		"eqeqeq": 1,
		"key-spacing": 1,
		"new-cap": 1,
		"no-alert": 0,
		"no-extra-boolean-cast": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-multi-spaces": 1,
		"no-process-exit": 1,
		"no-shadow": 1,
		"no-undef-init": 1,
		"no-underscore-dangle": 0,
		"no-unused-vars": 1,
		"no-use-before-define": 1,
		"quotes": [0, "single"],
		"space-infix-ops": 0
	}
}
